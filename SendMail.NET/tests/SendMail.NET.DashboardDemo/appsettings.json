{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "YourSecretKeyHere123!@#$%^&*()",
    "Issuer": "SendMail.NET.DashboardDemo",
    "Audience": "SendMail.NET.DashboardDemo",
    "ExpiryInMinutes": 60
  },
  "SendMail": {
    "Providers": {
      "Gmail": {
        "Type": "Smtp",
        "Host": "smtp.gmail.com",
        "Port": 587,
        "Username": "your-email@gmail.com",
        "Password": "your-app-password",
        "EnableSsl": true,
        "Quota": 500
      },
      "Office365": {
        "Type": "Smtp",
        "Host": "smtp.office365.com",
        "Port": 587,
        "Username": "your-email@outlook.com",
        "Password": "your-password",
        "EnableSsl": true,
        "Quota": 1000
      },
      "AwsSes": {
        "Type": "AwsSes",
        "Region": "us-west-2",
        "AccessKey": "your-access-key",
        "SecretKey": "your-secret-key",
        "Quota": 2000
      }
    }
  }
} 